<div class="mice_table_container">
  <table id="mice_list">
    <thead>
      <tr>
        <th>Name</th><th>Gold</th><th>Points</th>
      </tr>
    </thead>
    <tbody>
      <% mice.each do |mouse| %>
        <tr>
          <td><%= link_to  mouse.name, mouse_path(mouse) %></td>
          <td><%= mouse.gold %></td>
          <td><%= mouse.points %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>