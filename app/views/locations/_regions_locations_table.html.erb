<div class="locations_table_container">
<table id="locations_list">
  <thead>
    <tr>
      <th>Name</th><th>at Rank</th>
    </tr>
  </thead>
  <tbody>
      <% locations.each do |loc| %>
        <tr>
          <td><%= link_to loc.name, location_path(loc) %></td>
          <td><%= loc.rank.title %></td>
        </tr>
      <% end %>
  </tbody>
</table>
</div>